{
    "name": "moduleuserguide/laravel-user-guide-plugin",
    "description": "A Laravel plugin to manage and display user guides or documentation inside an existing Laravel project.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mari Selvan",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "ModuleUserGuide\\": "moduleuserguide/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ModuleUserGuide\\ModuleUserGuideServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
