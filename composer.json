{
    "name": "yourname/laravel-user-guide-plugin",
    "description": "Laravel 10 plugin for Module and User Guide CRUD",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/http": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelUserGuide\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelUserGuide\\LaravelUserGuideServiceProvider"
            ]
        }
    }
}
